@model IEnumerable<Infrastructure.Web.Domain.ModelsView.WeldingView>
@section easyui_css{
    <link rel="stylesheet" href="~/Style/EasyUI/easyui.css" />
}
<style>
    .datagrid-mask-msg {
        height: 36px !important;
    }
</style>
<!--Action boxes-->
<div class="row">
    <div class="col-sm-12">
        <form class="form-inline" id="SearchForm">
            <div>
                <input type="text" name="UploadFile" id="UploadFile" />
                <button type="button" id="btnSelect" class="btn btn-primary btn-sm" data-toggle="tooltip" data-placement="top" title="选择文件">选择文件</button>
                <div style="float:right">
                    <button type="button" id="btnAudit" class="btn btn-primary btn-sm" data-toggle="tooltip" data-placement="top" title="审核">审核</button>
                </div>
            </div>
        </form>
        <div class="clearfix"></div>

        <div class="widget-box">
            <div class="widget-content nopadding">
                <table class="table table-bordered table-hover data-table">
                    <thead>
                        <tr>
                            <th style="width:5%;text-align:center;">@T("序号")</th>
                            <th>@T("项目名称")</th>
                            <th>@T("梁段名称")</th>
                            <th>@T("DWG文件名")</th>
                            <th>@T("DWG上传进度")</th>
                            <th>@T("结果上传进度")</th>
                            <th>@T("任务状态")</th>
                            <th>@T("审核状态")</th>
                            <th>@T("创建人")</th>
                            <th>@T("创建日期")</th>
                            <th style="width:10%;">@T("操作")</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>


@section scripts_Foot
{
    <script src="~/Scripts/EasyUI/jquery.easyui.min.js"></script>
    <script src="http://static.runoob.com/assets/jquery-validation-1.14.0/dist/localization/messages_zh.js"></script>
    <script type="text/javascript">
        //$('[data-toggle="tooltip"]').tooltip();
        var popMenu;
        var IdCur;

        $(document).ready(function () {
            LEAP.Common.MainPop.options.dataTable = $('.data-table').dataTable(jQuery.extend(true, datatableSetting, {
                "ajax": {
                    "url": "GetList"
                },
                "paging": true,
                "info": false,
                "aoColumns": [
                    { "mData": 'Id' },
                    { "mData": 'ProjectId' },
                    { "mData": 'BeamId' },
                    { "mData": 'DWGFile' },
                    { "mData": 'DWGProcess' },
                    { "mData": 'ResultProcess' },
                    { "mData": 'TaskStatus' },
                    { "mData": 'AuditStatus' },
                    { "mData": 'CreatedUserID' },
                    { "mData": 'CreatedDate' },
                ],
                "columnDefs": [
                    {
                        "targets": 10,
                        "render": function (data, type, full, meta) {
                            return '<a href="#" onclick="LEAP.Common.MainPop.ShowUpdateInfo(\'' + full.Id + '\');return false;" class="artDailog btn btn-info  btn-xs" style="margin-right:4px;" data-toggle="tooltip" data-placement="top" title="焊材详情统计"><i class="fa fa-pencil"></i></a>' +
                                '<a href="#" onclick="LEAP.Common.MainPop.RowClick(\'' + full.Id + '\',2);return false;" class="btn btn-danger btn-xs" id="btnDelete" data-toggle="tooltip" data-placement="top" title="定额数据统计">' +
                                '<i class="fa fa-trash-o"></i></a>';
                        }
                    }],
                fnDrawCallback: function () {
                    $('[data-toggle="tooltip"]').tooltip();
                }
            }));
            //inital select2
            $('.select2').css("width", "362px").select2({ allowClear: true });
        });
    </script>
}

